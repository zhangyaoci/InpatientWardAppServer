<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="domain.Hospitalization" table="hospitalization" schema="impatientwardapp">
        <id name="hospitalId" column="hospitalId">
            <generator class="native" ></generator>
        </id>
        <property name="startTime" column="startTime"/>
        <property name="endTime" column="endTime"/>
        <property name="room" column="room"/>
        <property name="remarks" column="remarks"/>
        <many-to-one name="nurse" class="domain.Nurse" column="nurseId"></many-to-one>
        <many-to-one name="doctor" class="domain.Doctor" column="doctorId"></many-to-one>
        <many-to-one name="patient" class="domain.Patient" column="patientId"></many-to-one>
    </class>
</hibernate-mapping>